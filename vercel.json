{
  "version": 2,
  "builds": [
    {
      "src": "deploy-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/deploy-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "deploy-server.js": {
      "maxDuration": 30
    }
  },
  "installCommand": "npm install --legacy-peer-deps --omit=optional"
}